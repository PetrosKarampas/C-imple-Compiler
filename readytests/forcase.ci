program ForCase

		declare x,k;
		declare count,mult;
	
	# main #
	{
		count := 0;
		input(k);
		x := 100;
		forcase
			case (k>1000) 
			{
				count := count + 1;
				x := x*30;
				k := 69;
			}
			case (k<500) 
			{
				x:=x-100;
			}
			default 
			{
				x:=x+5;
			}
		;
		while ( count = x)
		{
			mult := mult * count;
			count := count + 10;
			
		};
		print(mult);
	}.
